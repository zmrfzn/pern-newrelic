{
  "name": "@pern-monorepo/backend",
  "version": "1.0.0",
  "description": "Node.js Restful CRUD API with Node.js, Express and PostgreSQL",
  "main": "server.js",
  "scripts": {
    "start": "env-cmd -f ./.env node server",
    "load": "npx load-generator",
    "initialize": "npx sequelize-cli db:create && npx sequelize-cli db:migrate && npx sequelize-cli db:seed:all",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "nodejs",
    "express",
    "rest",
    "api",
    "postgresql"
  ],
  "license": "ISC",
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.18.1",
    "load-generator": "^1.3.0",
    "pg": "^8.8.0",
    "pg-hstore": "^2.3.4",
    "pm2": "^5.2.0",
    "sequelize": "^6.27.0",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "env-cmd": "^10.1.0",
    "sequelize-cli": "^6.6.0"
  }
}
